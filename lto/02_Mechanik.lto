               <maxima><![CDATA[// Implementierung funktioniert nur mit MAXIMA, nicht mit Parser
// vexz(r, F) := r[1] * F[2] - r[2] * F[1]
vexz(r, F) := vget(r,0) * vget(F,1) - vget(r,1) * vget(F,0)

// Null-Vektoren
M0 : [0Nm, 0Nm, 0Nm]
F0 : [0N, 0N, 0N]

// Beträge der Kräfte
FGabs : m * g
FNabs : FGabs * cos(alpha)
FTabs : FGabs * abs(sin(alpha))

// Kraftvektoren der Gewichtskraft
FNvec : [0N, -FNabs, 0N]
FTvec : [-FTabs, 0N, 0N]
FGvec : FNvec + FTvec

// Abstsandsvektoren
rVvec : [a + b, 0m, 0m]
rGvec : [a, c, 0m]

// Kraftvektoren
FVvec : [0N, FVy, 0N]
FHvec : [FHx, FHy, 0N]

// Drehmomentgleichung erfordert Maxima statt Parser
equMz : vexz(rGvec,FGvec) + vexz(rVvec, FVvec) = 0Nm
// Kraftgleichung
equFx : vget(FVvec,0) + vget(FHvec,0) + vget(FGvec,0) = 0N
equFy : vget(FVvec,1) + vget(FHvec,1) + vget(FGvec,1) = 0N

// FHy : solvevalue(equMz, [FHy], [FHy])
FVysol : solvevalue([equMz, equFx, equFy], [FVy, FHx, FHy], FVy)
FHxsol : solvevalue([equMz, equFx, equFy], [FVy, FHx, FHy], FHx)
FHysol : solvevalue([equMz, equFx, equFy], [FVy, FHx, FHy], FHy)

// Rückeinsetzen in Vektoren
FVvec : ev(FVvec, FVy = FVysol)
FHvec : ev(FHvec, FHx = FHxsol, FHy = FHysol)
]]></maxima>
