               <maxima><![CDATA[// Implementierung funktioniert nur mit MAXIMA, nicht mit Parser
// vexz(r, F) := r[1] * F[2] - r[2] * F[1]
vexz(r, F) := vget(r,0) * vget(F,1) - vget(r,1) * vget(F,0)

// Null-Vektoren
M0 : [0Nm, 0Nm, 0Nm]
F0 : [0N, 0N, 0N]

// Beträge der Kräfte
FGabs : m * g
FNabs : FGabs * cos(alpha)
FTabs : FGabs * abs(sin(alpha))

// Kraftvektoren der Gewichtskraft
FNvec : [0N, -FNabs, 0N]
FTvec : [FTabs, 0N, 0N]
FGvec : FNvec + FTvec

// Abstsandsvektoren
rRvec : [-b, -d, 0m]
rGvec : [-(a + b), -d + c, 0m]

// Kraftvektoren
FRvec : [0N, FRy, 0N]
FKvec : [FKx, FKy, 0N]

// Drehmomentgleichung erfordert Maxima statt Parser
equMz : vexz(rGvec,FGvec) + vexz(rRvec, FRvec) = 0Nm
// Kraftgleichung
equFx : vget(FRvec,0) + vget(FKvec,0) + vget(FGvec,0) = 0N
equFy : vget(FRvec,1) + vget(FKvec,1) + vget(FGvec,1) = 0N

FRysol : solvevalue([equMz, equFx, equFy], [FRy, FKx, FKy], FRy)
FKxsol : solvevalue([equMz, equFx, equFy], [FRy, FKx, FKy], FKx)
FKysol : solvevalue([equMz, equFx, equFy], [FRy, FKx, FKy], FKy)

// Rückeinsetzen in Vektoren
FRvec : ev(FRvec, FRy = FRysol)
FKvec : ev(FKvec, FKx = FKxsol, FKy = FKysol)
]]></maxima>
